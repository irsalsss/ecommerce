# Production Environment Variables
NODE_ENV=production
PORT=3001

# Database - Use strong passwords in production!
POSTGRES_PASSWORD=change-this-in-production
DATABASE_URL=postgresql://postgres:change-this-in-production@postgres:5432/ecommerce_db

# Redis
REDIS_URL=redis://redis:6379

# JWT - Use a strong secret in production!
JWT_SECRET=change-this-to-a-very-strong-secret-in-production
JWT_EXPIRES_IN=7d

# API
API_PREFIX=api
